- semantic_form_for @teacher_exam do |f|
  = f.input :name
  = f.input :question_number
  = f.input :start_date, :include_blank => true, :required => false, :minute_step => 15
  = f.input :duration
  = f.input :teacher_questions, :collection => @current_user.teacher_questions, :label_method => :content, :include_blank => false
  - f.buttons do
    = f.commit_button "Commit"

